{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "address"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "addresses"
    },
    "address_title": {
      "type": "string",
      "required": true
    },
    "address_full_address": {
      "type": "text",
      "required": true
    },
    "address_postal_code": {
      "type": "biginteger",
      "required": true
    },
    "address_phone": {
      "type": "biginteger"
    }
  }
}
